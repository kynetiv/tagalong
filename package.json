{
  "name": "tagalong",
  "version": "0.1.0",
  "description": "HTML templating done right",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha",
    "bundle": "browserify bundle.js > tagalong.js",
    "minify": "uglifyjs tagalong.js > tagalong.min.js",
    "build": "npm run bundle && npm run minify",
    "watch": "watchy -w index.js,bundle.js -- npm run build"
  },
  "author": {
    "name": "Shawn Allen",
    "url": "https://github.com/shawnbot"
  },
  "repository": "shawnbot/tagalong",
  "license": "CC0",
  "devDependencies": {
    "browserify": "^10.2.4",
    "jsdom": "^3.1.2",
    "mocha": "^2.2.5",
    "uglifyjs": "^2.4.10",
    "watchy": "^0.6.2"
  },
  "dependencies": {}
}
